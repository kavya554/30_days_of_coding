#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    double mc;
    int tip,tax;
    scanf("%lf",&mc);
    scanf("%d",&tip);
    scanf("%d",&tax);
    double y=mc*tip/100;
    double z=mc*tax/100;
    //int total=mc+((tip/100)*mc)+((tax/100)*mc);
   
    int x=(int)round(mc+y+z);
    printf("%d",x);
    return 0;
    
}
