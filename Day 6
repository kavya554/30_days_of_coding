#include <stdio.h>
#include<string.h>
#include<stdlib.h>
#include<math.h>
void oddeven(char s[])
{
    for(int i=0;i<strlen(s);i++)
    {
        if(i%2==0)
        {
            printf("%c",s[i]);
        }
        
    }
    printf(" ");
    for(int i=0;i<strlen(s);i++)
    {
        if(i%2!=0)
        {
            printf("%c",s[i]);
        }
        
    }
     printf("\n");

}
int main()
{
    char s[10000];
    int n;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%s",s);
        oddeven(s);
    }
    
    
    return 0;
}
